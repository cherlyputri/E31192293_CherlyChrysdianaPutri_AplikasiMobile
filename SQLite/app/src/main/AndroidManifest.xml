<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Biodata"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLite">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BuatBiodata"
            android:label="Insert Biodata">
        </activity>
        <activity android:name=".LihatBiodata"
            android:label="View Biodata">
        </activity>
        <activity android:name=".UpdateBiodata"
            android:label="Update Biodata">
        </activity>
    </application>

</manifest>